{
    "name": "php-tailors/phpunit-regexp",
    "description": "PCRE assertions for PHPUnit-based tests",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
      "issues": "https://github.com/php-tailors/phpunit-extensions/issues",
      "source": "https://github.com/php-tailors/phpunit-extensions"
    },
    "authors": [
        {
            "name": "PaweÅ‚ Tomulik",
            "email": "ptomulik@meil.pw.edu.pl"
        }
    ],
    "require": {
        "php": ">=7.3",
        "php-tailors/phpunit-common": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.3"
    },
    "autoload": {
      "psr-4" : {
        "Tailors\\PHPUnit\\" : "src/"
      }
    },
    "autoload-dev": {
      "psr-4" : {
        "Tailors\\PHPUnit\\" : "tests/src/"
      }
    }
}
